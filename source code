<html>
    <script>
        var n1=20;
        var n2=20;
        function add(){
           alert(n1+n2);
        }
        function sub(){ 
           alert(n1-n2);
        }
        function mul(){
           alert(n1*n2);
        }
        function div(){
           alert(n1/n2);
        }
        </script>
        <button onclick=add()>Add </button>
        <button onclick=sub()>sub </button>
        <button onclick=mul()>mul </button>
        <button onclick=div()>div </button>
        <body>
            </body>
    </html>
